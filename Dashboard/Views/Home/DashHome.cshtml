@model Dashboard.Models.BaseObject


<!DOCTYPE html>

<html>
<head>
    <title>Dashboard Home</title>
    <link rel="stylesheet" type="text/css" href="~/content/reset.css">
    <link rel="stylesheet" type="text/css" href="~/content/styles.css">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://fonts.googleapis.com/css?family=Lato" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/weather-icons/2.0.9/css/weather-icons-wind.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/weather-icons/2.0.9/css/weather-icons-wind.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/weather-icons/2.0.9/css/weather-icons.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/weather-icons/2.0.9/css/weather-icons.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/weather-icons/2.0.9/font/weathericons-regular-webfont.eot" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/weather-icons/2.0.9/font/weathericons-regular-webfont.svg" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/weather-icons/2.0.9/font/weathericons-regular-webfont.ttf" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/weather-icons/2.0.9/font/weathericons-regular-webfont.woff" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/weather-icons/2.0.9/font/weathericons-regular-webfont.woff2" />
</head>
<body>
    <header>
        <div class="nav-wrapper">
            <nav class="navbar-nav">
                <div class="page-wrap">
                    <ul>
                        <li><a href="#">First</a></li>
                        <li><a href="#">Second</a></li>
                        <li><a href="#">Third</a></li>
                    </ul>
                </div>
            </nav>
        </div>
    </header>
    <section class="dash-wrap">
        <div class="page-wrap">
            <div id="resize" class="container">
                <div class="weather-container">
                    <div class="weather-data-wrap">
                        <p id="city"></p>
                        <hr>
                        <p id="temp_f"><span>Temp</span></p>
                        <p id="feelslike_f"><span>Feels Like</span></p>
                    </div>
                    <i id="weather-icon"></i>
                </div>
            </div>
            <div id="resize" class="container">
                <div class="habitica-container">
                    <div class="habitica-container-wrap">
                        <p id="profile">To Do's</p>
                        <hr>
                        <div id="todos"></div>
                    </div>
                    <i class="fa fa-check-circle-o" aria-hidden="true"></i>
                </div>
            </div>
            <div class="container">
                <div class="pocket-container">
                    <div class="pocket-container-wrap">
                        <p class="pocket-header">Pocket Articles</p>
                        <hr />
                        <div id="pocket-data"></div>
                    </div>
                </div>
            </div>

        <!--Handle Bars Testing -->

        <div id="mike-data"></div>


        </div> @* Page Wrap *@
    </section> @* Dash Wrap *@
</body>

</html>


<script id="pocket-template" type="text/x-handlebars-template">
    @*<div class="testing">
        {{#each list}}
        <a href="{{resolved_url}}">{{resolved_title}}</a>
        {{/each}}
    </div>*@

    <div class="testing">
        <p>{{Title}}</p>
        <p>{{URL}}</p>
    </div>
</script>




<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
<script type="text/javascript" src="https://npmcdn.com/draggabilly@2.1/dist/draggabilly.pkgd.min.js"></script>
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/handlebars.js/4.0.11/handlebars.min.js"></script>
<script type="text/javascript" src="~/content/scripts.js"></script>